{
  "version": 2,
  "functions": {
    "api.py": {
      "runtime": "@vercel/python@4.3.1",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "DATABASE_URL": "$DATABASE_URL",
    "FRONTEND_URL": "$FRONTEND_URL",
    "SECRET_KEY": "$SECRET_KEY",
    "ALGORITHM": "$ALGORITHM",
    "ACCESS_TOKEN_EXPIRE_MINUTES": "$ACCESS_TOKEN_EXPIRE_MINUTES"
  }
}